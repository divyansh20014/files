public class JsonMapping {
		
    private String Action = "", ActionDateTime = "", CustomerId = "", AccountId = "", ChannelDailyLimit = "", Balance = "", TransactionAmountInLocalCurrency = "", TransactionAmountInTransactionCurrency = "", TransactionCurrencyCode = "", ChannelDeviceId = "", CountryISO2 = "", ClientIPAddress = "", BeneficiaryType = "", BeneficiaryCountry = "", BeneficiaryAccountId = "", CardStatus = "", MaskedCardNumber = "", RBADeviceIdType = "", DeviceName = "", MerchantId = "", MerchantName = "", CreditCardLimit = "", TransactionDescription = "", BeneficiaryName = "", BeneficiaryId = "", OrgName = "", BeneficiaryBankName = "", EFMSChannelReferenceNumber = "", ChannelCode = "", tranDate = "", tranTime = "";

public void jsonMapping(Exchange exchange) {
	System.out.println("Started mapping logic for Bill payment");
    // Get the incoming JSON from the Camel exchange
