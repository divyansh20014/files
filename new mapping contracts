            }

  
        try {
            MessageAPI api = MessageAPI.getDefault();
            MessageApiEncoding apie = MessageApiEncoding.getDefault();
            LengthCodec lc = LengthCodec.PREPEND2;

            // Create transaction
            Transaction txn = new Transaction(apie);

    
